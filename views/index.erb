<% @students.each do |student| %>
  <p>First_Name <%= student.first_name() %></p>
  <p>Second_Name <%= student.second_name() %></p>
  <p>House <%= student.house() %></p>
  <p>Age <%= student.age() %></p>
  <hr />
<% end %>

<% @houses.each do |house| %>
  <p>Name <%= house.name() %></p>
  <p>URL<%= house.url() %></p>
  <hr />
<% end %>
